<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
  <name>Collection of SVG files One per root layer</name>
  <id>org.atc24.output.tar_leaf_layers</id>
  <dependency type="extension">org.inkscape.output.svg.inkscape</dependency>
  <output priority='7'>
    <extension>.tar</extension>
    <mimetype>application/tar</mimetype>
    <filetypename>Deepest nested layers as Separate SVG (*.tar)</filetypename>
    <filetypetooltip>Every leaf layer split into it's own svg file and collected as a tape archive (tar file)</filetypetooltip>
    <dataloss>false</dataloss>
  </output>
  <script>
    <command location="inx" interpreter="python">tar_leaf_layers.py</command>
    <helper_extension>org.inkscape.output.svg.inkscape</helper_extension>
  </script>
</inkscape-extension>
